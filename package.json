{
  "name": "ai-skills-manager",
  "version": "1.6.0",
  "description": "AI Skills Manager - CLI tool for managing Claude Code Agent Skills",
  "author": "lwndev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lwndev/ai-skills-manager.git"
  },
  "homepage": "https://github.com/lwndev/ai-skills-manager#readme",
  "bugs": {
    "url": "https://github.com/lwndev/ai-skills-manager/issues"
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "skills",
    "cli",
    "anthropic"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "asm": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.19.6"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "quality": "npm run lint && npm run test:coverage && npm run audit",
    "prepare": "husky",
    "prepublishOnly": "npm run quality && npm run build",
    "preversion": "npm run build && npm test && npm run lint",
    "version": "git add package.json package-lock.json",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "commander": "^14.0.2",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.5.3"
  }
}
